			"label": "00 - go mod tidy",
			"type": "shell",
			"command": "go",
			"args": [
				"mod",
				"tidy"
			],
			"group": "build"
		},
		{
			"label": "00 - go build",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongtree"
			},
			"command": "go",
			"args": [
				"build"
			],
			"group": "build"
		},
	]
