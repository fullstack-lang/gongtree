{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "02 - run gongtree -unmarshallFromCode=stage.go -marshallOnCommit=stage",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongtree"
			},
			"command": "go",
			"args": [
				"run",
				"main.go",
				"-unmarshallFromCode",
				"stage.go",
				"-marshallOnCommit",
				"stage"
			],
			"group": "build"
		},
		{
			"label": "03 - lsof -i tcp:8080 ",
			"type": "shell",
			"command": "lsof -i tcp:8080 ",
			"group": "build"
		},
		{
			"label": "03 - ng build",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongtree; ng build",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "03 - ngbuild",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongtree; ngbuild",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "04 - ng serve --host=127.0.0.1",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongtree; ng serve --host=127.0.0.1",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "04 - ngserve",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongtree; ngserve",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "01 - gongc gongtree",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": []
		},
		{
			"label": "01 - gongc gongtree -skipNg",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipNg"
			]
		},
		{
			"label": "01 - gongc gongtree -skipGoModCommands",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipGoModCommands"
			]
		},
		{
			"label": "01 - gongc gongtree -skipNg -skipGoModCommands",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipNg",
				" -skipGoModCommands"
			]
		},
		{
			"label": "01 - gongc gongtree -skipNpmInstall -skipNg -skipGoModCommands",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipNpmInstall",
				"-skipNg",
				"-skipGoModCommands"
			]
		},
